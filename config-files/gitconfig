[alias]
	st = status
	co = commit -m
	ch = checkout
	cm = checkout master
	cb = checkout -b
	unstage = reset HEAD --
  	undo = checkout --
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	lg = log --pretty=format:'%Cred%h%Creset %C(bold)%cr%Creset %Cgreen<%an>%Creset %s' --max-count=30
[user]
	email = ""
	name = ""
[core]
	editor = code --wait
